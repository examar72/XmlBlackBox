<?xml version="1.0" encoding="utf-8" ?>
<TEST version="1.1" 
	name="ExampleVersion37_003"
	xmlns="http://www.xmlblackbox.org/xsd/" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.xmlblackbox.org/xsd/ ../../../../../../../resources/xsd/xmlblackbox_1_7.xsd">


    <!-- Example to do a selenium web driver navigation with HtmlUnitDriver-->
    <SELENIUM-WEB-DRIVER-CLIENT version="1.1" name="Example37 with SELENIUM WEB DRIVER CLASS" browser-type="HTMLUNIT">
            <NAVIGATION-FILE class="org.xmlblackbox.test.functional.examples.v37.selenium.WebApplicationTestWebDriver">
                <PARAMETERS>
                        <PARAMETER name="EXAMPLE_WEB_URL" value="${EXAMPLE_WEB_URL@inputProperties}"/>
                </PARAMETERS>
            </NAVIGATION-FILE>
    </SELENIUM-WEB-DRIVER-CLIENT>

    <SELENIUM-WEB-DRIVER-CLIENT version="1.1" name="Example37 with SELENIUM WEB DRIVER CLASS Next Page" browser-type="HTMLUNIT">
            <NAVIGATION-FILE class="org.xmlblackbox.test.functional.examples.v37.selenium.WebApplicationTestWebDriverNextPage"/>
    </SELENIUM-WEB-DRIVER-CLIENT>


    <!-- Example to do a selenium web driver navigation with PhantomjsDriver-->
    <SELENIUM-WEB-DRIVER-CLIENT browser-session="Phantom" version="1.1" name="Example37 with SELENIUM WEB DRIVER CLASS" browser-type="PHANTOMJS">
            <NAVIGATION-FILE class="org.xmlblackbox.test.functional.examples.v37.selenium.WebApplicationTestWebDriver">
                <PARAMETERS>
                        <PARAMETER name="EXAMPLE_WEB_URL" value="${EXAMPLE_WEB_URL@inputProperties}"/>
                </PARAMETERS>
            </NAVIGATION-FILE>
    </SELENIUM-WEB-DRIVER-CLIENT>

    <SELENIUM-WEB-DRIVER-CLIENT browser-session="Phantom" version="1.1" name="Example37 with SELENIUM WEB DRIVER CLASS Next Page" browser-type="PHANTOMJS">
            <NAVIGATION-FILE class="org.xmlblackbox.test.functional.examples.v37.selenium.WebApplicationTestWebDriverNextPage"/>
    </SELENIUM-WEB-DRIVER-CLIENT>

</TEST>

